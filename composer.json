{
  "name": "tiamo/spss",
  "description": "SPSS is a php-based implementation of IBM SPSS Statistics Standard",
  "keywords": [
    "SPSS",
    "IBM",
    "PSPP",
    "Statistics",
    "DataSet",
    "por",
    "sav",
    "reader",
    "writer",
    "cli",
    "cli-app"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Korniienko Vladyslav",
      "email": "vk.tiamo@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-mbstring": "*",
    "ext-bcmath": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "ergebnis/phpstan-rules": "^0.15.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpstan/phpstan": "^0.12.31",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0",
    "rector/rector": "^0.7.41",
    "squizlabs/php_codesniffer": "^3",
    "thecodingmachine/phpstan-strict-rules": "^0.12.0"
  },
  "autoload": {
    "psr-4": {
      "SPSS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SPSS\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "scripts": {
    "lint": "rector process src && php-cs-fixer fix -v",
    "test:lint": "php-cs-fixer fix -v --dry-run && rector process src --dry-run",
    "test:types": "phpstan analyse --ansi --memory-limit=0",
    "test:unit": "phpunit --no-coverage",
    "test": [
      "@test:lint",
      "@test:types",
      "@test:unit"
    ]
  }
}
